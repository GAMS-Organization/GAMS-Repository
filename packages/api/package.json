{
  "name": "api",
  "version": "1.0.0",
  "description": "REST api for GAMS",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:transpile": "tsc && node dist/server.js",
    "start": "nodemon -e ts  --exec \"yarn run dev:transpile\"",
    "build": "rm -Rf dist/ && tsc && rm -Rf build/ && tsc && webpack --mode production",
    "serve": "node build/index.js",
    "linter:check": "eslint 'src/**/*.ts'",
    "prettier:check": "prettier -c 'src/**/*.ts'",
    "prettier:run": "prettier --write 'src/**/*.ts'",
    "prettier:checkJSON": "prettier -c 'src/**/*.json'",
    "prettier:runJSON": "prettier --write 'src/**/*.json'",
    "migration:generate": "../../node_modules/.bin/typeorm migration:generate -n",
    "migration:make": "../../node_modules/.bin/typeorm migration:create -n",
    "migration:migrate": "ts-node ../../node_modules/typeorm/cli.js migration:run",
    "migration:rollback": "ts-node ../../node_modules/typeorm/cli.js migration:revert",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/config": "^0.0.34",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/es6-shim": "^0.31.39",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/inversify": "^2.0.33",
    "@types/morgan": "^1.7.35",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.48.1",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "joi-to-swagger": "^3.2.0",
    "morgan": "^1.9.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/node": "^12.11.7",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint": "^6.6.0",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "inversify": "^5.0.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.19.5",
    "mocha": "^6.1.4",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-schedule": "^2.0.0",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.4.0",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.16",
    "winston": "^3.2.1",
    "winston-sentry-log": "^1.0.11"
  }
}
